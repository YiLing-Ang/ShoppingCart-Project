@{
    ViewData["Title"] = "Cart";
    List<Cart> carts = (List<Cart>)ViewData["carts"];
    int userCartQty = (int)ViewData["userCartQty"];
}
@foreach(Cart cart in carts)
{
    <p>@cart.Product.Name</p>
    <p>@cart.Product.Price</p>
    <p>@cart.Quantity</p>
    <br />
}